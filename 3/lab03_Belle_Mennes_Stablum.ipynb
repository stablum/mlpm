      "        print \"P(\\\\alpha):\", p_alpha\n",
      "        print \"P(\\\\mu):\", p_mu\n",
      "        print \"P(\\\\tau):\", p_tau\n",
      "        print \"Q(\\\\alpha):\", q_alpha\n",
      "        print \"Q(\\\\tau):\", q_tau\n",
      "        \n",
      "        return L\n",
      "    \n",
      "    \n",
      "    def fit(self, D):\n",
      "        pass"
